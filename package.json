{
    "name": "hera-info",
    "version": "1.0.0",
    "description": "",
    "keywords": [],
    "license": "ISC",
    "author": "",
    "main": "index.js",
    "scripts": {
        "analyze": "ANALYZE=1 umi build",
        "build": "umi build",
        "commit-msg": "pig-fabric --hooks=commit-msg",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx --quiet && stylelint 'src/**/*.{less,css}'",
        "lint-fix": "eslint src --ext .js,.jsx,.ts,.tsx --quiet --fix && stylelint 'src/**/*.{less,css}' --fix",
        "pig-fabric:install": "pig-fabric install",
        "pre-commit": "pig-fabric --hooks=pre-commit && lint-staged",
        "pre-merge-commit": "pig-fabric --hooks=pre-merge-commit",
        "prepare": "npx husky install",
        "start": "npm run test",
        "test": "cross-env NODE_ENV_APP=test umi dev"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,less,css,md}": "pretty-quick --staged",
        "*.{js,jsx,ts,tsx}": "eslint --fix --quiet",
        "*.{css,less}": "stylelint"
    },
    "dependencies": {
        "@pigjs/request": "^1.0.2",
        "@pigjs/utils": "^1.0.5",
        "antd-mobile": "^5.28.2",
        "axios": "^1.3.4",
        "umi": "^4.0.63"
    },
    "devDependencies": {
        "@pigjs/fabric": "^1.0.1",
        "@types/react": "^18.0.30",
        "babel-plugin-import": "^1.13.6",
        "cross-env": "^7.0.3",
        "eslint": "7.32.0",
        "husky": "8.0.2",
        "lint-staged": "13.0.3",
        "postcss-px-to-viewport": "^1.1.1",
        "prettier": "2.7.1",
        "pretty-quick": "3.1.3",
        "stylelint": "13.13.1"
    }
}
